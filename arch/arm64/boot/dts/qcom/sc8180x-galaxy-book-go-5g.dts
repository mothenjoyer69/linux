/ {
	model = "Samsung Galaxy Book Go 5G";
	compatible = "samsung,galaxy-book-go-5g", "qcom,sc8180x";

	// backlight: backlight {
	// 	compatible = "pwm-backlight";
	// 	pwms = <&pmc8180c_lpg 4 1000000>;
	// 	enable-gpios = <&pmc8180c_gpios 8 GPIO_ACTIVE_HIGH>;

	// 	pinctrl-names = "default";
	// 	pinctrl-0 = <&bl_pwm_default_state>;
	// };

	chosen { };

	reserved-memory { // Values from Galaxy Book S, placeholder only
		rmtfs_mem: memory@85500000 {
			compatible = "qcom,rmtfs-mem";
			reg = <0x0 0x85500000 0x0 0x200000>;
			no-map;

			qcom,client-id = <1>;
			qcom,vmid = <15>;
		};

		wlan_mem: memory@8bc00000 {
			reg = <0x0 0x8bc00000 0x0 0x180000>;
			no-map;
		};

		mpss_mem: memory@8d800000 {
			reg = <0x0 0x8d800000 0x0 0x3000000>;
			no-map;
		};

		adsp_mem: memory@90800000 {
			reg = <0x0 0x90800000 0x0 0x1c00000>;
			no-map;
		};

		gpu_mem: memory@98715000 {
			reg = <0x0 0x98715000 0x0 0x2000>;
			no-map;
		};

		cdsp_mem: memory@98900000 {
			reg = <0x0 0x98900000 0x0 0x1400000>;
			no-map;
		};
	};

	vph_pwr: vph-pwr-regulator {
		compatible = "regulator-fixed";
		regulator-name = "vph_pwr";
		regulator-min-microvolt = <3700000>;
		regulator-max-microvolt = <3700000>;
	};

	vreg_s4a_1p8: pm8150-s4 {
		compatible = "regulator-fixed";
		regulator-name = "vreg_s4a_1p8";

		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;

		regulator-always-on;
		regulator-boot-on;

		vin-supply = <&vph_pwr>;
	};

	/* uses AUO B140HTN02.0 Panel */
	panel {
		compatible = "boe,<xxx>"; //fix
		//no-hpd; ???

		//backlight = <&backlight>; TODO

		ports {
			port {
				panel_in: endpoint {
					remote-endpoint = <&mdss_edp_out>;
				};
			};
		};
	};
};

